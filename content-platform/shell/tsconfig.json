{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "@tab-contract": ["../tab-contract/src"],
      "*": ["./@mf-types/*"]
    }
  },
  "include": ["src/**/*", "./@mf-types/*"],
  "exclude": ["node_modules", "dist"]
}
