{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./@mf-types"],
    "paths": {
      "@tab-contract": ["../tab-contract/src"]
    }
  },
  "include": ["src/**/*", "@mf-types/**/*.d.ts"]
}
